<h2><%= @customer.last_name %><%= @customer.first_name %>さんの会員情報</h2>

<h4>会員ID</h4>
<%= @customer.id %>
<%= form_with model: [:admin, @customer], local:true do |f| %>
<h4>氏名</h4>
<%= f.text_field :last_name %> <%= f.text_field :first_name %>
<h4>フリガナ</h4>
<%= f.text_field :last_name_kana %> <%= f.text_field :first_name_kana %>
<h4>郵便番号</h4>
<%= f.text_field :postal_code %>
<h4>住所</h4>
<%= f.text_field :address %>
<h4>電話番号</h4>
<%= f.text_field :telephone_number %>
<h4>メールアドレス</h4>
<%= f.email_field :email %>
<h4>会員ステータス</h4>

<%= f.submit '変更を保存' %>
<% end %>